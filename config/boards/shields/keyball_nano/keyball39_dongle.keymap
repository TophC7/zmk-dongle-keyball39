 *
 * Minimal keymap for USB dongle (no physical keys)
 */

#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        /* Dongle has no physical keys - single empty layer required by ZMK */
        default_layer {
            bindings = <&none>;
        };
    };
};